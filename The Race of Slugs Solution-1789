#include<stdio.h>

int main()
{
    int n,x,max=0;
    while(scanf("%d",&n) != EOF)
    {

        for(int i=1; i<=n; i++)
        {
            scanf("%d",&x);

            if(x>max)
            {
                max  = x;
            }
        }
        
        if(max<10) printf("1\n");
        else if(max>10 && max<20) printf("2\n");
        else if(max>= 20)printf("3\n");
        max =0;
    }
}

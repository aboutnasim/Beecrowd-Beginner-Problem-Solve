#include<stdio.h>
int main()
{

    int N, i;
    float x, y;
    scanf("%d",&N);



    for(i=1; i<=N; i++)
    {
        scanf("%f %f", &x, &y);

        if(y==0)
            printf("divisao impossivel\n");

        else
            if(x>0 || x<0 && y>0 || y<0)

                printf("%0.1f\n",x/y);

            else
                printf("0.0\n");


    }

    return 0;
}

#include<stdio.h>
int main()
{
    int x,y,i,sum1,sum2;
    scanf("%d %d", &x,&y);

    if(x>y){
        for(i=y; i<=x; i++){
            if(i%13!=0)
                sum1 = sum1 + i;
        }
        printf("%d\n", sum1);
    }

    else if(y>x){
        for(i=x; i<=y; i++){
            if(i%13!=0)
                sum2 = sum2 + i;
        }
        printf("%d\n", sum2);
    }


    return 0;
}

#include<stdio.h>
int main()
{

    int n;
    scanf("%d",&n);

    for(int i=1; i<=n; i++)
    {
        for(int j=1; j<=3; j++)

        {
            if(j==2)
            {
                printf("%d ",i*i);
            }
            else if(j==3)
            {
                printf("%d",(i*i)*i);
            }

            else
            {
                printf("%d ",i);
            }
        }
        printf("\n");

    }
}
